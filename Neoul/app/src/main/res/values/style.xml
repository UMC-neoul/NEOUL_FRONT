<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="expandedBarBrandTitleStyle">
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#1A1C1E</item>
    </style>

    <style name="expandedBarStoryTitleStyle">
        <item name="android:textSize">24sp</item>
        <item name="android:lines">2</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="UnderLine">
        <item name="android:textColor">#5A5F66</item>
        <item name="android:background">@drawable/underline</item>
    </style>

    <style name="MyPageUnderLine">
        <item name="android:textColor">#5A5F66</item>
        <item name="android:background">@drawable/my_page_underline</item>
    </style>

    <style name="DivisionLine">
        <item name="android:textColor">#5A5F66</item>
        <item name="android:background">@drawable/division_line</item>
    </style>

    <style name="TextBox">
        <item name="android:textColor">#E2E2E6</item>
        <item name="android:background">@drawable/textbox</item>
    </style>

    <style name="SuccessFailBox">
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@drawable/success_fail_box</item>
    </style>

    <style name="SuccessFailBoxVer2">
        <item name="android:textColor">#5E8FEC</item>
        <item name="android:background">@drawable/success_fail_box_ver2</item>
    </style>

    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">#fcfcfc</item>
        <item name="colorPrimaryDark">#889dfe</item>
        <item name="colorAccent">#ff7b31</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>



</resources>